type Query {
    getCharacterById(id: Int): CharacterDTO
    saveCharacterById(id: Int): SaveCharacterDTO
}

type CharacterDTO {
    id: Int
    name: String
    status: String
    species: String
    type: String
    episodeCount: Int
    origin: OriginDTO
}

type OriginDTO {
    name: String
    url: String
    dimension: String
    residents: [String]
}

type SaveCharacterDTO {
    id: String
}